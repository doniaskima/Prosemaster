{
  "printWidth": 80,           
     
  "tabWidth": 2,              
  
  "useTabs": false,               
  
  "semi": true,                  
  
  "singleQuote": false,          
  
  "quoteProps": "as-needed",     
  
  "jsxSingleQuote": false,        
  
  "trailingComma": "none",      
  
  "bracketSpacing": true,      
  
  "arrowParens": "avoid",        
  
  "endOfLine": "auto",        
  
  "htmlWhitespaceSensitivity": "css",  
  
  "vueIndentScriptAndStyle": false,    
  
  "proseWrap": "preserve"       
  
}
